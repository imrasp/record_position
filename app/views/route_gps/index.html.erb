<p id="notice"><%= notice %></p>

<h1>Route Gps</h1>

<table>
  <thead>
    <tr>
      <th>Mission</th>
      <th>Time</th>
      <th>Lat</th>
      <th>Lon</th>
      <th>Alt</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @route_gps.each do |route_gp| %>
      <tr>
        <td><%= route_gp.mission %></td>
        <td><%= route_gp.time %></td>
        <td><%= route_gp.lat %></td>
        <td><%= route_gp.lon %></td>
        <td><%= route_gp.alt %></td>
        <td><%= link_to 'Show', route_gp %></td>
        <td><%= link_to 'Edit', edit_route_gp_path(route_gp) %></td>
        <td><%= link_to 'Destroy', route_gp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Route Gp', new_route_gp_path %>
